{
	"name": "react-ant-design",
	"version": "0.0.1",
	"private": true,
	"dependencies": {
		"@ant-design/icons": "^4.3.0",
		"@reduxjs/toolkit": "^1.5.0",
		"@testing-library/jest-dom": "^5.11.8",
		"@testing-library/react": "^11.2.3",
		"@testing-library/user-event": "^12.6.0",
		"@types/history": "^4.7.8",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.20",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-redux": "^7.1.15",
		"@types/react-router-dom": "^5.1.7",
		"@types/redux-auth-wrapper": "^2.0.9",
		"@types/redux-mock-store": "^1.0.2",
		"@types/validatorjs": "^3.15.0",
		"antd": "^4.10.2",
		"antd-dayjs-webpack-plugin": "^1.0.5",
		"axios": "^0.21.1",
		"connected-react-router": "^6.8.0",
		"customize-cra": "^1.0.0",
		"history": "^5.0.0",
		"less": "^3.13.1",
		"less-loader": "^7.1.0",
		"react": "^17.0.1",
		"react-app-rewired": "^2.1.8",
		"react-dom": "^17.0.1",
		"react-error-boundary": "^3.1.0",
		"react-redux": "^7.2.2",
		"react-router-dom": "^5.2.0",
		"react-scripts": "4.0.1",
		"redux-auth-wrapper": "^3.0.0",
		"redux-mock-store": "^1.5.4",
		"redux-persist": "^6.0.0",
		"redux-persist-transform-encrypt": "^3.0.1",
		"redux-saga": "^1.1.3",
		"typescript": "^4.1.3",
		"validatorjs": "^3.22.1",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"eject": "react-scripts eject",
		"stylelint": "stylelint '**/*{.css,.scss}'",
		"stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
		"commit": "cz",
		"---------------------------------------;": "-= TEST SECTION =-",
		"test": "react-app-rewired test",
		"test:report": "react-app-rewired test --coverage --watchAll",
		"test:text-summary": "react-app-rewired test --coverage --coverageReporters='text-summary' --watchAll "
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "commitizen.js"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js'",
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"./**/*.{css,scss}": [
			"stylelint --fix"
		],
		"./**/*.{json,md}": [
			"prettier --write"
		],
		"./**/*.{js,jsx,ts,tsx}": [
			"eslint --debug src/ --fix",
			"jest --bail --findRelatedTests"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"babel-plugin-import": "^1.13.3",
		"commitizen": "^4.2.2",
		"cz-customizable": "^6.3.0",
		"eslint": "^7.17.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"git-cz": "^4.7.6",
		"husky": "^4.3.7",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"stylelint": "^13.8.0",
		"stylelint-config-airbnb": "^0.0.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-order": "^4.1.0",
		"stylelint-prettier": "^1.1.2",
		"stylelint-scss": "^3.18.0",
		"ts-jest": "^26.4.4"
	}
}
